[core]
	repositoryformatversion = 0
	filemode = true
	bare = true
[hooks]
        fromdomain = adacore.com
        mailinglist = git-hooks-ci@example.com
        # Set the maxcommitemails value to a ridiculously low value
        # for testing purposes.
        maxcommitemails = 3
        # We want no commit emails on master.  It should mean that
        # maxcommitemails has no influence over that branch.
        noemails = refs/heads/master
